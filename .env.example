# FastAPI Config
PORT=8080
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000

# Azure Storage
AzureWebJobsStorage=DefaultEndpointsProtocol=https;AccountName=...;AccountKey=...;EndpointSuffix=core.windows.net
AZURE_BRONZE_CONTAINER=bronze
AZURE_SILVER_CONTAINER=silver

# SQL Database
SQL_SERVER=tcp:yourserver.database.windows.net,1433
SQL_DATABASE=yourdb
SQL_USERNAME=youruser
SQL_PASSWORD=yourpassword
SQL_DRIVER=ODBC Driver 18 for SQL Server
SQL_ENCRYPT=true
SQL_TRUST_CERT=false
SQL_CONN_TIMEOUT=30

# AI Services
AZURE_AI_EXTRACTOR_ENDPOINT=https://your-resource.cognitiveservices.azure.com/
AZURE_AI_EXTRACTOR_KEY=yourkey

# OpenAI (optional if using Azure OpenAI)
OPENAI_API_KEY=sk-...

# Environment
ENV=production
LOG_LEVEL=info
